{
  "name": "nova-backend-e2e",
  "$schema": "../../node_modules/nx/schemas/project-schema.json",
  "projectType": "application",
  "implicitDependencies": ["nova-backend"],
  "targets": {
    "e2e": {
      "executor": "@nx/jest:jest",
      "outputs": ["{workspaceRoot}/coverage/{e2eProjectRoot}"],
      "options": {
        "jestConfig": "apps/nova-backend-e2e/jest.config.cts",
        "passWithNoTests": true
      },
      "dependsOn": ["nova-backend:build", "nova-backend:serve"]
    }
  }
}
