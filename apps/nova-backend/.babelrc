{
  "presets": [
    [
      "@babel/preset-env",
      {
        "targets": {
          "node": "18"
        }
      }
    ],
    "@babel/preset-typescript"
  ],
  "plugins": [
    [
      "@babel/plugin-proposal-decorators",
      {
        "version": "2023-05"
      }
    ],
    "@babel/plugin-proposal-class-properties",
    "@babel/plugin-transform-class-static-block",
    "babel-plugin-transform-typescript-metadata"
  ]
}
